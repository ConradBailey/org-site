<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
  <channel>
    <title>{{blog-name}}</title>
    <link>{{site-url}}/{{blog-url}}</link>
    <description>{{description}}</description>
    <pubDate>{{current-time}}</pubDate>
    <lastBuildDate>{{current-time}}</lastBuildDate>
    <docs>http://www.rssboard.org/rss-specification</docs>
    <generator>Org-Site (https://github.com/ConradBailey/org-site)</generator>
    {{#posts}}
    <item>
      <title>{{title}}</title>
      <link>{{site-url}}/{{blog-url}}/{{nav-url}}</link>
      <description><![CDATA[{{description}}]]></description>
      <pubDate>{{creation}}</pubDate>
      <guid>{{site-url}}/{{blog-url}}/{{nav-url}}</guid>
    </item>
    {{/posts}}
  </channel>
</rss>